# Environment Configuration Template
# Copy this to .env and fill in your actual values

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Firebase Configuration
# Place your Firebase service account JSON files in the project root

# Google Gemini AI
GEMINI_API_KEY=your_gemini_api_key_here

# ElevenLabs AI Voice (REQUIRED for high-quality voice)
# Get your API key from: https://elevenlabs.io/app/settings/api-keys
# The key provided in the original request appears to be invalid/expired
# You MUST get a new key from your ElevenLabs account
ELEVENLABS_API_KEY=sk_get_your_own_key_from_elevenlabs_dashboard
ELEVENLABS_AGENT_ID=optional_agent_id_for_conversational_ai

# API Configuration
API_BASE_URL=https://your-production-domain.com
PORT=5000

# Node Environment
NODE_ENV=development
